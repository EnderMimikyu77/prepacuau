#include <iomanip>
#include <iostream>
#include <string>
using namespace std;
int main (){
    
    int numero;
    
    cout<<"Tabla de multiplos \n";
    cout<<"Elige un numero \n";
    cin>>numero;
    for(int i = 1; i < 11; i++){
        cout<< i << " * " << numero << " = " << i * numero <<endl;
        
        
    }
    
    
    return 0;
    
}
